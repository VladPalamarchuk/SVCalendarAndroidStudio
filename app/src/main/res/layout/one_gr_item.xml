<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/gr_event_name_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/gr_event_name_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#cecece" />

        <TextView
            android:id="@+id/gr_event_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="#cecece" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/gr_event_start_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/gr_event_start_date_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#cecece" />

        <TextView
            android:id="@+id/gr_event_start_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="#cecece" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/gr_event_end_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/gr_event_end_date_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#cecece" />

        <TextView
            android:id="@+id/gr_event_end_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="#cecece" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/gr_event_push_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/gr_event_push_date_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#cecece" />

        <TextView
            android:id="@+id/gr_event_push_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="#cecece" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/gr_event_categ_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/gr_event_category_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:textColor="#cecece" />

        <TextView
            android:id="@+id/gr_event_category"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
   
            android:textColor="#cecece" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/gr_event_location_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/gr_event_location_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
       
            android:textColor="#cecece" />

        <TextView
            android:id="@+id/gr_event_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
     
            android:textColor="#cecece" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/gr_event_info_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/gr_event_info_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
   
            android:textColor="#cecece" />

        <TextView
            android:id="@+id/gr_event_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
      
            android:textColor="#cecece" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/gr_event_file_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/gr_event_file_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
       
            android:textColor="#cecece" />

        <TextView
            android:id="@+id/gr_event_file"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
    
            android:textColor="#cecece" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/gr_event_people_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:orientation="vertical" >
    </LinearLayout>

</LinearLayout>