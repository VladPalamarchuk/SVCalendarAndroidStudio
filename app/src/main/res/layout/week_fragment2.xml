<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout 
        android:id="@+id/week_fragmant2_bacground"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.5">
        
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/week_fragment2_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#cdcdcd" >

            <ImageView
                android:id="@+id/week_fragment2_search_image"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:padding="5dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:scaleType="centerCrop"
                android:src="@drawable/search"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/week_fragment2_open_left_menu"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:scaleType="centerCrop"
                android:src="@drawable/back_to_menu" />

            <Spinner
                android:id="@+id/week2_fragment_spinner"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:padding="2dp"
                android:spinnerMode="dropdown"
                android:visibility="invisible" />
            
            
            
            
            <ImageView
                android:id="@+id/week_fragment2_add_event"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"
                android:layout_alignParentRight="true"
                android:padding="5dp"
                android:scaleType="fitXY"
                android:src="@drawable/add_event_top_base_theme" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/week_fragment2_root_rel"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/week_fragment2_pn_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true" />

                        <ListView
                            android:id="@+id/week_fragment2_pn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/week_fragment2_pn_title" >
                        </ListView>

                        <ProgressBar
                            android:id="@+id/progres_pn"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_centerInParent="true" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/week_fragment2_vt_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true" />

                        <ListView
                            android:id="@+id/week_fragment2_vt"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/week_fragment2_vt_title" >
                        </ListView>

                        <ProgressBar
                            android:id="@+id/progres_vt"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_centerInParent="true" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/week_fragment2_sr_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true" />

                        <ListView
                            android:id="@+id/week_fragment2_sr"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/week_fragment2_sr_title" >
                        </ListView>

                        <ProgressBar
                            android:id="@+id/progres_sr"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_centerInParent="true" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/week_fragment2_ch_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true" />

                        <ListView
                            android:id="@+id/week_fragment2_ch"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/week_fragment2_ch_title" >
                        </ListView>

                        <ProgressBar
                            android:id="@+id/progres_ch"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_centerInParent="true" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/week_fragment2_pt_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true" />

                        <ListView
                            android:id="@+id/week_fragment2_pt"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/week_fragment2_pt_title" >
                        </ListView>

                        <ProgressBar
                            android:id="@+id/progres_pt"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_centerInParent="true" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/week_fragment2_sb_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true" />

                        <ListView
                            android:id="@+id/week_fragment2_sb"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/week_fragment2_sb_title" >
                        </ListView>

                        <ProgressBar
                            android:id="@+id/progres_sb"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_centerInParent="true" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <TextView
                            android:id="@+id/week_fragment2_vs_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true" />

                        <ListView
                            android:id="@+id/week_fragment2_vs"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/week_fragment2_vs_title" >
                        </ListView>

                        <ProgressBar
                            android:id="@+id/progres_vs"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_centerInParent="true" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/week_fragment2_mounth_rel"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <RelativeLayout
                            android:id="@+id/week_frgament2_name_day_mounth"
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_alignParentTop="true" >
                        </RelativeLayout>

                        <ProgressBar
                            android:id="@+id/progres_mounth"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_centerInParent="true"
                            android:visibility="invisible" />

                        <RelativeLayout
                            android:id="@+id/week_frgament2_mounth"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_above="@+id/week_fragment2_mounth_names_rel"
                            android:layout_below="@+id/week_frgament2_name_day_mounth" >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical" >

                                <ImageView
                                    android:id="@+id/first_row"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/plashka"
                                    android:visibility="invisible" />

                                <ImageView
                                    android:id="@+id/second_row"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/plashka"
                                    android:visibility="invisible" />

                                <ImageView
                                    android:id="@+id/three_row"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/plashka"
                                    android:visibility="invisible" />

                                <ImageView
                                    android:id="@+id/four_row"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/plashka"
                                    android:visibility="invisible" />

                                <ImageView
                                    android:id="@+id/five_row"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/plashka"
                                    android:visibility="invisible" />

                                <ImageView
                                    android:id="@+id/six_row"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/plashka"
                                    android:visibility="invisible" />
                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/week_fragment2_mounth_names_rel"
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:layout_alignParentBottom="true" >

                            <TextView
                                android:id="@+id/week_fragment2_name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true" />

                            <ImageView
                                android:id="@+id/week_fragment2_levo"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="10dp"
                                android:layout_toLeftOf="@+id/week_fragment2_name"
                                android:src="@drawable/strelka_levo" />

                            <ImageView
                                android:id="@+id/week_fragment2_pravo"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@+id/week_fragment2_name"
                                android:src="@drawable/strelka_pravo" />
                        </RelativeLayout>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

</RelativeLayout>